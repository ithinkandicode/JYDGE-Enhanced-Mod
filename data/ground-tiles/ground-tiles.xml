<?xml version="1.0" encoding="iso-8859-1"?>

<root>

	<!--
	
	All jpg and png files in ground-tiles/ folder are automatically appended to this list with following default values:
	
	id="ground-tiles/filename"
	tags=""
	physics_material="CONCRETE" (first one in physics-materials.xml definition)
	z_offset="0"
	
	tags
	SKIP_RENDERING
    
    forced_tile_pattern_marker can be used to convert tileset pattern "F" into this character (mostly likely just "0").
	
	-->

	<array id="GROUND_TILES">

		<node id="ground-tiles/floor-transparent.png" tags="SKIP_RENDERING" />
	
		<node id="ground-tiles/floor-metal-large.jpg" physics_material="CONCRETE" />
		<node id="ground-tiles/floor-metal-brushed.jpg" physics_material="CONCRETE" />
		<node id="ground-tiles/black.jpg" physics_material="CONCRETE" z_offset="-122" />
		<node id="ground-tiles/asphalt-2x2.jpg" physics_material="CONCRETE" ___forced_tile_pattern_marker="0" />
		<node id="ground-tiles/floor-concrete-tiled-rough.jpg" physics_material="CONCRETE" ___forced_tile_pattern_marker="0" />
		<node id="ground-tiles/grass-2x2.jpg" forced_tile_pattern_marker="0" />
		<node id="ground-tiles/dirt-2x2.jpg" forced_tile_pattern_marker="0" />

	</array>


</root>